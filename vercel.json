{
  "rewrites": [
    {
      "source": "/((?!apartment-.*\\.jpg|favicon\\.ico|robots\\.txt|_redirects).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/apartment-.*\\.jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
